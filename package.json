{
  "name": "reaviz",
  "version": "14.2.5",
  "description": "Data Visualization using React",
  "scripts": {
    "start": "set  NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 9010 -s public",
    "build-storybook": "set  NODE_OPTIONS=--openssl-legacy-provider && build-storybook -s public",
    "copy": "cp CNAME storybook-static/",
    "build": "rollup -c",
    "lint": "eslint --ext js,ts,tsx",
    "lint:fix": "eslint --ext js,ts,tsx --fix src",
    "test": "jest",
    "test:ci": "jest -collectCoverage=true --reporters=default --reporters=jest-junit",
    "ts:check": "tsc --noEmit",
    "prettier": "prettier --loglevel warn --write 'src/**/*.{ts,tsx,js,jsx}'",
    "chromatic": "chromatic --project-token=d5ql53y3nwr --exit-zero-on-changes"
  },
  "source": "src/index.ts",
  "main": "dist/index.cjs.js",
  "exports": "./dist/index.esm.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.js",
  "style": "dist/index.css",
  "typings": "dist/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reaviz/reaviz.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "reactjs",
    "d3.js",
    "d3",
    "dataviz",
    "charts",
    "charting",
    "svg",
    "bar-chart",
    "axis",
    "brush",
    "scatter-plot",
    "area-chart",
    "sparkline",
    "line-chart",
    "pie-chart",
    "sankey",
    "gauge",
    "donut-chart",
    "pan",
    "zoom",
    "radial",
    "brush",
    "heatmap",
    "calendar-heatmap",
    "legends",
    "venn",
    "euler",
    "venn-diagram",
    "treemap"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/reaviz/reaviz/issues"
  },
  "homepage": "https://github.com/reaviz/reaviz#readme",
  "dependencies": {
    "@upsetjs/venn.js": "^1.3.0",
    "big-integer": "1.6.49",
    "calculate-size": "1.1.1",
    "chroma-js": "^2.4.2",
    "classnames": "^2.3.2",
    "d3-array": "^3.0.4",
    "d3-format": "^3.0.1",
    "d3-geo": "^3.0.1",
    "d3-hierarchy": "^3.0.1",
    "d3-interpolate": "^3.0.1",
    "d3-sankey": "^0.12.3",
    "d3-scale": "^4.0.2",
    "d3-shape": "^3.0.1",
    "d3-time": "^3.0.0",
    "ellipsize": "^0.2.0",
    "framer-motion": "7.8.0",
    "human-format": "^0.11.0",
    "invert-color": "^2.0.0",
    "memoize-bind": "^1.0.3",
    "memoize-one": "^5.2.1",
    "rdk": ">=6.4.4",
    "reablocks": ">=3.6.20",
    "react-cool-dimensions": "^2.0.7",
    "react-fast-compare": "^3.2.1",
    "transformation-matrix": "^2.9.0"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@juggle/resize-observer": "^3.3.1",
    "@mdx-js/react": "1.6.22",
    "@rollup/plugin-json": "^4.1.0",
    "@storybook/addon-docs": "6.4.20",
    "@storybook/addon-google-analytics": "6.2.9",
    "@storybook/addon-knobs": "6.4.0",
    "@storybook/addon-storysource": "6.4.20",
    "@storybook/addons": "6.4.20",
    "@storybook/react": "6.4.20",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/classnames": "^2.3.1",
    "@types/d3-geo": "^3.0.2",
    "@types/d3-hierarchy": "^3.0.2",
    "@types/d3-sankey": "^0.11.2",
    "@types/d3-scale": "^4.0.2",
    "@types/d3-shape": "^3.0.2",
    "@types/d3-time": "^3.0.0",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/topojson-client": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "autoprefixer": "^9.8.5",
    "chromatic": "^6.10.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.7",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.3",
    "jest-junit": "^12.3.0",
    "lint-staged": "^10.5.4",
    "moment": "^2.29.1",
    "postcss-nested": "^4",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.6.2",
    "react": "^18.2.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^18.2.0",
    "remark-codesandbox": "^0.10.0",
    "rollup": "^2.38.4",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-postcss-modules": "^2.0.2",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "0.26.0",
    "storybook-css-modules-preset": "^1.1.1",
    "topojson-client": "^3.1.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.1.2",
    "world-atlas": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/.storybook/",
      "<rootDir>/storybook-static/",
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/src/**/*(*.)@(spec|test).[t]s?(x)"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!d3-array|d3-shape|d3-path|internmap|d3-interpolate|d3-scale|d3-format|d3-time).+\\.js$"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "./coverage"
        }
      ]
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "watchPlugins": [],
    "setupFilesAfterEnv": [
      "./test/mockEnv.ts"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --ext js,ts,tsx --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest-junit": {
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}"
  }
}
